IF EOF()
	DO pantalla
	RETURN
ENDIF
ACTIVATE WINDOW wind_0
SCATTER MEMVAR

@ 00, 60 SAY EstPAC()
@ 01, 26 SAY m.CodPAC
@ 01, 50 SAY m.FecPAC
@ 02, 26 SAY m.TipVta
@ 02, 32 SAY VAL_PARA(m.TipVta,'TIPVTA','A',32,20)
@ 03, 26 SAY m.CodCre
