XCTA=SPACE(10)

XCTA=SPACE(10)
FOR i=1 TO 11
	@5,5 GET XCTA
	READ
	FOR j=1 TO 4
		XCTA=ALLTRIM(XCTA)
		APPEND BLANK
		REPLACE CODREP WITH 'F-2'
		REPLACE CUENTA WITH PADR(XCTA,10,'0')
		REPLACE FILA WITH ALLTRIM(STR(I))
		REPLACE COLUMNA WITH ALLTRIM(STR(J))
	ENDFOR
	XCTA=SPACE(10)
ENDFOR

CANCEL

FOR j=1 TO 16
	XCTA=ALLTRIM(XCTA)
	APPEND BLANK
	REPLACE CODREP WITH 'F-1'
	REPLACE CUENTA WITH PADR(XCTA,10,'0')
	REPLACE COLUMNA WITH ALLTRIM(STR(J+4))
ENDFOR
