SET EXCLU OFF
SELE 1
INDEX ON CODFTE+NUMREF TO A1 FOR TIPDOC='O/S'
USE HOJCON INDEX A1
SELE 2
INDEX ON CODFTE+NUMOS TO A2 
USE ORDSER INDEX A2
SET RELA TO CODFTE+NUMOS INTO 1
GO TOP
SET DEVI TO PRIN
SET PRIN ON
SCAN FOR ORDSER.CODPRV # HOJCON.CODPRV AND HOJCON.CODPRV#'0000' AND !EMPTY(HOJCON.CODPRV)
? 'OS=',ORDSER.NUMOS,'/',ORDSER.CODFTE,' PRV= ',ORDSER.CODPRV,'|','HJ.OS=',HOJCON.NUMREF,'/',HOJCON.CODFTE,' HJ.PRV=',HOJCON.CODPRV,' HJ.CT=',HOJCON.NUMMES,HOJCON.NUMHC
*REPLA HOJCON.CODPRV WITH ORDSER.CODPRV
ENDSCAN
SET PRIN OFF
SET DEVI TO SCREE