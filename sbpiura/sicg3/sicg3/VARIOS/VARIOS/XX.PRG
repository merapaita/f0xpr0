PARAMETERS xAno

cPath = "h:\sicgdata\DATA"+ALLTRIM(STR(xAno,4))
cFile = "OS"+STR(xAno,4)

SET PATH TO (cPath)
SET EXCLU OFF
SET ECHO OFF
SET DELE ON

clos data
USE ORDSER IN 1 ORDER TAG ORDSER1
USE ITEOS1 IN 2 ORDER TAG IteOS11
USE Auxil  IN 3 ORDER TAG Auxil1

SET RELATION TO periodo+ numos INTO ITEOS1 ADDITIVE
SET RELATION TO "20"+ codprv   INTO AUXIL  ADDITIVE

SET FILTER TO ESTADO#'99' AND codprv='0337'
*SET FILTER TO CODPRV="0341" AND ESTADO#'99'
*SET FILTER TO (CODPRV="0133" OR CODPRV="0188" OR CODPRV="0270" OR CODPRV="0342" OR CODPRV="0155" OR CODPRV="0156" OR CODPRV="0341") AND ESTADO#'99'

COPY TO (cFile) FIELD PERIODO,NUMOS,FECOS,AUXIL.DESCRI,ESTADO,VALTOT,DETALLE