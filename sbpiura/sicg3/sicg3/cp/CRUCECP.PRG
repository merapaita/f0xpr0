CLOSE DATA
SET PATH TO ..\DATA
SET DELE ON
USE compag    IN 1   order tag COMPAG1  ALIAS COMPA
USE ITECP     IN 2   order tag ITECP1   ALIAS ITECP
SELE ITECP 
VREC = RECNO()
GO TOP
DO WHILE !EOF()
	vnummes = ALLTRIM(nummes)
	vnumcp  = numcp
	vcodctc = ALLTRIM(CODCTC)
	SELE COMPA
	SEEK VNUMMES+VNUMCP+VCODCTC
	IF !FOUND()
		SELE ITECP
		REPLACE CODCAL WITH SUBSTR(CODCAL,1,21)+'X'
	ENDIF	
	SELE ITECP
	SKIP
	IF EOF()
		EXIT
	ENDIF	
ENDDO
RETURN