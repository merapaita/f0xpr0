PROCEDURE salmenu
*----------------
STORE  YESNO(' Estamos Terminando la sesin ') TO in_regrau
IF in_regrau
	CLOSE DATA
	CLEAR WINDOWS
	RELEASE ALL
	CLEAR
	*=BorCon()
	=SavCon(2)
	IF !worker
		QUIT
	 ELSE
		CANCEL
	ENDIF
 ELSE
	RETURN
ENDIF


