CLOS ALL
SET TALK OFF
SET SAFETY OFF
SET DEFA TO H:\SIS98\DATA
USE CHEQUE2	IN 1 ORDER CHEQUE1
USE DETCTA	IN 2 ORDER DETA1

STORE SPACE(14) TO VCTA
STORE SPACE(2) TO VMES
@12,10 SAY "INGRESE MES : " GET VMES PICTURE '99' valid(VMES$'010203040506070809101112')
READ
@14,10 SAY "INGRES CUENTA :" GET VCTA
READ

SELE DETCTA
SET FILTER TO CODCTC=VCTA AND MES=VCTA

SELE CHEQUE2
SET FILTER TO CODCTC=VCTA AND Estado<>'99' 
GO TOP
DO WHILE !EOF() 
	CHQ=CHEQUE2.NUMCHQ
	SELE DETCTA
	GO TOP
	SEEK CHQ
	IF FOUND()
		SELE CHEQUE2
		REPLA PAGADO WITH 'S'
		REPLA MESP WITH VMES
	ELSE
	SELE CHEQUE2
	REPLA PAGADO WITH 'N'
	ENDIF
	SELE CHEQUE2
	SKIP
ENDDO
SET DEFA TO H:\SIS98\TESO1

